{
  "name": "portfolio",
  "private": true,
  "description": "Developer portfolio built with Gatsby",
  "version": "0.2.0",
  "author": "Jeff Maxwell",
  "homepage": "https://el-jefe.me",
  "license": "MIT",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "NODE_ENV=production gatsby build && node scripts/generate-csp-hashes.js",
    "build:netlify": "npm install --legacy-peer-deps && NODE_ENV=production gatsby build --no-uglify",
    "build:secure": "NODE_ENV=production GENERATE_SOURCEMAP=false gatsby build && node scripts/build-secure.js && node scripts/generate-csp-hashes.js",
    "build:analyze": "NODE_ENV=production ANALYZE=true gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "storybook build",
    "build:all": "npm run build && npx storybook build -o public/storybook && cd docs-site && npm install --legacy-peer-deps && npm run build && cp -r build ../public/docs",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:update-snapshots": "playwright test --update-snapshots",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "test:storybook": "start-server-and-test storybook http://localhost:6007 cypress:run",
    "test:storybook:open": "start-server-and-test storybook http://localhost:6007 cypress:open"
  },
  "overrides": {
    "cookie": ">=0.7.0",
    "@apollo/client": {
      "rxjs": "^7.8.0"
    }
  },
  "dependencies": {
    "@apollo/client": "^4.1.4",
    "babel-plugin-styled-components": "^2.1.4",
    "gatsby": "^5.14.5",
    "gatsby-adapter-netlify": "^1.2.0",
    "gatsby-plugin-google-gtag": "^5.14.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-offline": "^6.14.0",
    "gatsby-plugin-preconnect": "^1.4.0",
    "gatsby-plugin-react-helmet-async": "^1.2.3",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-plugin-styled-components": "^6.14.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-prismjs": "^7.16.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-ffmpeg": "^0.5.5",
    "gatsby-transformer-remark": "^6.16.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "motion": "^12.23.12",
    "prismjs": "^1.30.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-animated-cursor": "^2.11.2",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^5.5.0",
    "styled-components": "^6.1.19",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@chromatic-com/storybook": "^5.0.0",
    "@playwright/test": "^1.58.1",
    "@storybook/addon-a11y": "^10.2.7",
    "@storybook/addon-docs": "^10.2.7",
    "@storybook/react-vite": "^10.2.7",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.3",
    "assert": "^2.1.0",
    "axe-core": "^4.11.1",
    "axe-playwright": "^2.2.2",
    "babel-jest": "^30.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "cheerio": "^1.1.2",
    "crypto-browserify": "^3.12.1",
    "cypress": "^15.10.0",
    "cypress-axe": "^1.7.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "events": "^3.3.0",
    "fastestsmallesttextencoderdecoder": "^1.0.22",
    "gatsby-plugin-brotli": "^2.1.0",
    "gatsby-plugin-eslint": "^4.0.4",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.32",
    "gatsby-plugin-zopfli": "^2.0.0",
    "html-minifier": "^4.0.0",
    "html-minifier-terser": "^7.2.0",
    "http-server": "^14.1.1",
    "https-browserify": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "netlify-plugin-cache": "^1.0.3",
    "netlify-plugin-submit-sitemap": "^0.4.0",
    "object.assign": "^4.1.7",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "react-test-renderer": "^18.3.1",
    "start-server-and-test": "^2.1.3",
    "storybook": "^10.2.7",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vite": "^7.3.1",
    "vite-plugin-svgr": "^4.5.0",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.98.0",
